{
  "expo": {
    "name": "Nöbetçi Eczaneler",
    "slug": "nobetci-eczaneler",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.aliyapici.NobetciEczaneler",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Yakındaki nöbetçi eczaneleri görmek için konumunuza erişim sağlanması gerekiyor.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "dev.aliyapici.NobetciEczaneler"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2e1d2680-3656-4e41-9266-4782c8caf038"
      }
    }
  }
}
